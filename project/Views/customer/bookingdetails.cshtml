
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>bookingdetails</title>
</head>
<body>
        <div>
            <table border="2px">
                <tbody>
                    @foreach (var item in Model)
                    {

                        <tr>
                            <td>@item.bookingid</td>

                            <td> @item.custid</td>

                            <td> @item.carid</td>

                            <td>@item.driverid</td>

                            <td>@item.startdate</td>

                            <td>@item.enddate</td>

                            <td> @item.totalprice</td>

                            <td> @item.fromroute</td>

                            <td> @item.toroute</td>

                            <td> @Html.ActionLink("Cancel", "Cancel")</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    
</body>
</html>
